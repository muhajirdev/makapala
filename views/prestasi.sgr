extends(src='layout.sgr')
  block(name='content')
    a(href='/'):h3 <- Back to Home

    section.container
      h1 Prestasi Makapala
      #prestasi-card-container
        .card
          table
            tr
              th Tahun
              th Prestasi
              th Kegiatan
            each(loop='item of data.contentful.prestasi')
              tr
                td {{ item.tahun }}
                td {{ item.prestasi }}
                td {{ item.kegiatan  }}

